class JeuDeCalculMental extends Program {
    
    int operande(){
	int nombre=(int)(random()*100);
	return nombre;
    }

    String affichage(int a, int b){
	String res=operande() + " + " + operande();
	return res;
    }

    int[][] tableauEntier(){
	int[][] tab=new int[3][3];
	for(int i=0; i<length(tab,1); i++){
	    for(int j=0; i<length(tab,2); j++){
		tab[i][j]=operande();
		tab[(int)(random()*length(tab,1))][(int)(random()*length(tab,2))]= 42 + 100;
	    }
	}
	return tab;
    }

    void testAffichageTableau(){
	int[][] tab = new int[][]{
	    {1,2,3},
	    {4,5,6},
	    {7,8,9}
	};
	assertEquals(" 1 2 3\n 4 5 6\n 7 8 9\n",affichageTableau(tab));
    }

    String affichageTableau(int[][] tab){
	String res="";
	for(int i=0; i<length(tab,1); i++){
	    for(int j=0; j<length(tab,2); j++){
		res+=" " + tab[i][j];
	    }
	    res+="\n";
	}
	return res;
    }

    /* void algorithm(){
	println (affichage(operande(), operande()));
	println (affichageTableau());
	}*/
}
